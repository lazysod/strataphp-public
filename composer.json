{
    "name": "lazysod/strataphp",
    "description": "A modular PHP framework with PSR-4 autoloading.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "htdocs/app/",
            "App\\Controllers\\": "htdocs/controllers/",
            "App\\Modules\\Admin\\Controllers\\": "htdocs/modules/admin/controllers/",
            "App\\Modules\\Admin\\Models\\": "htdocs/modules/admin/models/",
            "App\\Modules\\Links\\Controllers\\": "htdocs/modules/links/controllers/",
            "App\\Modules\\User\\Controllers\\": "htdocs/modules/user/controllers/",
            "App\\Modules\\Contact\\Controllers\\": "htdocs/modules/contact/controllers/"
        }
    },
    "require": {
        "phpmailer/phpmailer": "^6.10",
        "vlucas/phpdotenv": "^5.6",
        "twig/twig": "^3.21"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.13"
    }
}
