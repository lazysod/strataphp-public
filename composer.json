{
    "name": "barry/simple-php-framework",
    "description": "A modular PHP framework with PSR-4 autoloading.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "htdocs/app/",
            "Modules\\": "htdocs/modules/"
        }
    },
    "require": {
        "phpmailer/phpmailer": "^6.10"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.13"
    }
}
